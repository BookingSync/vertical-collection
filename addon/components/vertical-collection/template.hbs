{{#each virtualComponents key="id" as |item index| ~}}
  {{~#if item.isOccludedContent ~}}
    {{{unbound item.element}}}
  {{~else~}}
    {{~unbound item.upperBound~}}
    {{~yield item.content item.index ~}}
    {{~unbound item.lowerBound~}}
  {{~/if~}}
{{~/each}}

{{#if shouldYieldToInverse}}
  {{yield to="inverse"}}
{{/if}}
